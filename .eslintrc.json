{
  "extends": "@rocketseat/eslint-config/react",

  // Add your custom rules here
"plugins": [
    "import"
  ],
  "rules": {
    "camelcase": "off",
    "import/order": ["error", { // Ordena automaticamente as importações
      "groups": [["builtin", "external"], "internal", ["parent", "sibling", "index"]],
      "newlines-between": "always"
    }]
  }
}